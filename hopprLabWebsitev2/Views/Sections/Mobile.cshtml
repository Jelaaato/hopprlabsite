
@{
    ViewBag.Title = "Mobile";
}

<div class="row noGutter">
    <div class="col-xs-12 col-lg-12">
        <div class="mobileContainer">
            <center>
                <span style="line-height:70vh;">
                    <span class="headerText" style="color:white;"></span>
                </span>
            </center>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-12" style="padding-top:80px;padding-bottom:80px;">
            <center>
                <span class="headerTitle">OUR WORK</span>
            </center>
        </div>
    </div>
</div>

<div class="container" style="padding-bottom: 80px;">
    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 paddingBottom">
            <div class="wrapper">
                <img src="~/Content/Images/our_work/eventmanagermobile.jpg" class="img-responsive imageOne" />
                <div class="wrapper-overlay hidden-md hidden-sm hidden-xs">
                    <center>
                        <span class="wrapper-overlay-text">Event Manager</span>
                        <p class="wrapper-overlay-description" style="padding-top:30px;">
                            Create the best registration experience with automated on-site check in.
                        </p>
                    </center>
                </div>
                <div class="visible-md visible-sm visible-xs">
                    <br />
                    <center>
                        <span class="wrapper-overlay-text" style="color:black;">Event Manager</span><br />
                        <span class="wrapper-overlay-description" style="color:black;font-size:12px;">
                            Create the best registration experience with automated on-site check in.
                        </span>
                    </center>
                </div>
            </div>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 paddingBottom">
            <div class="wrapper">
                <img src="~/Content/Images/our_work/ahmcapp.jpg" class="img-responsive imageOne" />
                <div class="wrapper-overlay hidden-md hidden-sm hidden-xs">
                    <center>
                        <span class="wrapper-overlay-text">Asian Hospital App</span>
                        <p class="wrapper-overlay-description" style="padding-top:30px;">
                            Feel right at home as we provide a turn by turn directions to the hospital. Get in touch at ease as you can now quickly contact us directly. Get to know us more and have a look on our unwavering world class quality care in our website.
                        </p>
                    </center>
                </div>
                <div class="visible-md visible-sm visible-xs">
                    <br />
                    <center>
                        <span class="wrapper-overlay-text" style="color:black;">Asian Hospital App</span><br />
                        <span class="wrapper-overlay-description" style="color:black;font-size:12px;">
                            Feel right at home as we provide a turn by turn directions to the hospital. Get in touch at ease as you can now quickly contact us directly. Get to know us more and have a look on our unwavering world class quality care in our website.
                        </span>
                    </center>
                </div>
            </div>
        </div>
    </div>
</div>

